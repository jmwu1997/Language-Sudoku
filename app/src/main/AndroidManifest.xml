<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eta.sudoku">

    <application
        android:name=".SudokuApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name=".view.SelectorActivity"
            android:label="@string/title_activity_selector"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".view.VocabWeekListActivity"
            android:label=" "
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.VocabWeekActivity"
            android:label="Week By Week List"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.VocabFullActivity"
            android:label="@string/title_activity_full_vocab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.VocabActivity"
            android:label="@string/button_vocab"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.PuzzleActivity"
            android:theme="@style/AppThemeNoActionBar" />
        <activity
            android:name=".view.MenuActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>